{
  "name": "rong-form",
  "version": "0.1.0",
  "description": "Modern form management library for React powered by Mobx",
  "main": "./lib/index",
  "module": "./es/index",
  "scripts": {
    "start": "dumi dev",
    "test": "echo \"Error: no test specified\" && exit 1",
    "docs:build": "dumi build",
    "docs:deploy": "gh-pages -d .doc",
    "deploy": "npm run docs:build && npm run docs:deploy",
    "prettier": "prettier --write \"**/*.{js,jsx,tsx,ts,less,md,json}\"",
    "build": "father-build"
  },
  "files": [
    "lib",
    "es",
    "dist"
  ],
  "keywords": ["react","form","mobx"],
  "author": "Tingzhou Jia",
  "license": "MIT",
  "peerDependencies": {
    "react": "^16.8.0 || ^17.0.0",
    "react-dom": "^16.8.0 || ^17.0.0"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@babel/runtime": "^7.13.17",
    "async-validator": "^3.5.1",
    "mobx": "^6.2.0",
    "mobx-react": "^7.1.0",
    "mobx-state-tree": "^5.0.1",
    "react-is": "^17.0.2"
  },
  "devDependencies": {
    "@umijs/preset-react": "1.x",
    "@umijs/test": "^3.0.5",
    "babel-plugin-import": "^1.13.0",
    "deep-equal": "^2.0.3",
    "dumi": "^1.0.33",
    "father": "^2.30.4",
    "father-build": "^1.18.6",
    "fetch-jsonp": "^1.1.3",
    "gh-pages": "^3.0.0",
    "lint-staged": "^10.0.7",
    "prettier": "^1.19.1",
    "react": "^16.12.0",
    "rong-react": "^0.1.4"
  }
}
